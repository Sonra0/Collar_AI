{
  "name": "meeting-body-language-coach",
  "version": "1.0.0",
  "description": "Chrome extension for real-time body language feedback during Google Meet calls.",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "frame-recorder": "node tools/frame-recorder-server.mjs",
    "test": "node --test tests/*.test.mjs",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.{js,html,css}\""
  },
  "keywords": [
    "chrome-extension",
    "body-language",
    "meet",
    "ai-coach"
  ],
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^9.21.0",
    "prettier": "^3.5.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
